!----------!--------------------------------!--------!
! mod-list ! mod-keys                       ! alias  !
!----------!--------------------------------!--------!
! Shift    ! Shift_[LR]                     ! -      !
! Lock     ! Caps_Lock                      ! -      !
! Control  ! Control_[LR]                   ! -      !
! Mod1     ! Alt_[LR]                       ! Meta   !
! Mod2     !                                !        !
! Mod3     ! {ISO_Level3_Shift,Mode_switch} ! Altrgr !
! Mod4     ! Super_[LR]                     ! WinKey !
! Mod5     !                                !        !
!----------!--------------------------------!--------!

!----------------------------------------------------!
! bonus                                              !
!----------------------------------------------------!
! Hyper_[LR]                                         !
! Meta_[LR]                                          !
! Num_Lock                                           !
! Scroll_Lock                                        !
!----------------------------------------------------!
! xmodmap -pke > .Xmodmap_default && xev             !
!----------------------------------------------------!


! Control_L
! keycode 37 = ydiaeresis oslash NoSymbol
! keycode 37 = ISO_Level3_Shift NoSymbol ISO_Level3_Shift

!capslock
keycode 66 = ISO_Level3_Shift NoSymbol ISO_Level3_Shift
! keycode 66 = ydiaeresis oslash
! keycode 66 = Control_L NoSymbol Control_L

! winkey (Super)
keycode 133 = Control_L NoSymbol Control_L
keycode 134 = Control_R NoSymbol Control_R

! menu key? the weird on the right
keycode 135 = Alt_R Meta_R Alt_R Meta_R


clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add shift   = Shift_L Shift_R
! add lock    = Caps_Lock
add control = Control_L Control_R
add mod1    = Alt_L Alt_R
! add mod2    = Meta_L Meta_R
add mod3    = Num_Lock
! add mod4    = Super_L Super_R
! add mod5    = Mode_switch
